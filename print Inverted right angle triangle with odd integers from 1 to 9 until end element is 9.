import java.util.Scanner;
class Main
{
	public static void main(String args[])
	{
		Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        for(int i=0;i<n;i++){
           int num = 1+(2*i);
           for(int j=i;j<n;j++){
             System.out.print(num + " ");
             num += 2;
           }
          System.out.println();
       }
	}
}
