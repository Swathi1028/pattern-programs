import java.io.*;

class GFG {
    public static void main(String[] args) {
        int n = 5; // Number of rows and columns
        int i = 1;

        // Outer loop for rows
        while (i <= n) {
            int j = 1;

            // Inner loop for columns
            while (j <= n) {
                System.out.print("* ");
                j++;
            }

            // Move to the next line after printing a row
            System.out.println();
            i++;
        }
    }
}
